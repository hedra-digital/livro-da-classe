<!DOCTYPE html>
<html>
	<head>
				<!-- Meta -->
		<meta charset="utf-8">
		<meta name="viewport" content="width=device-width, minimum-scale=1.0, initial-scale=1.0, user-scalable=yes">
		<meta name="theme-color" content="#0068AE">
		<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">

		<!-- CSS -->
		<link rel="shortcut icon" sizes="32x32" href="/images/app-icon-32.png">
		<link rel="manifest" href="/manifest.json">
		<link href='https://fonts.googleapis.com/css?family=Roboto:400,300,500,700' rel='stylesheet' type='text/css'>

		<%= yield(:title).presence || content_tag(:title, "#{Publisher.get_current_app}") %>
		<%= stylesheet_link_tag    "scielo" %>
		<%= stylesheet_link_tag    "general" %>
		<%= javascript_include_tag "application" %>
		<%= favicon_link_tag %>
		<%= csrf_meta_tags %>
		<%= yield :meta_tags %>
	</head>
	<body>

		<!--[if lt IE 7]>
			<p class="chromeframe">You are using an <strong>outdated</strong> browser. Please <a href="http://browsehappy.com/">upgrade your browser</a> or <a href="http://www.google.com/chromeframe/?redirect=true">activate Google Chrome Frame</a> to improve your experience.</p>
		<![endif]-->

		<div class="wrap">
			

			<!-- Header -->
			<header class="main-header">
				<div class="navbar">
					<div class="navbar-inner navbar-top">
						<div class="navbar-logo" >
							<%= image_tag "scielo/logo-scielo.png", class: 'logo' %>
							<a href="#" class="logo">Livros</a>
							<div class="publish">Editoração Digital Online</div>
						</div>
						<div class="navbar-user">
							<label><a href="entrar.html">Acesse sua conta</a> ou <a href="cadastro.html">Cadastre-se</a></label>
						</div>
						<div class="language">
							<a href="#" class="lang-en" lang="en">English</a>
							<a href="#" class="lang-es" lang="es">Español</a>
						</div>
					</div>
					<div class="navbar-inner navbar-bottom">
						<ul class="nav">
							<li class="active"><a href="/#">Quem somos</a></li>
							<li><a href="/#servicos">Serviços</a></li>
							<li><a href="/#clientes">Parceiros</a></li>
							<li><a href="/#contato">Contato</a></li>
						</ul>
					</div>
				</div>
			</header>
			<div class="header-space"></div>


			
			<!-- Content -->
			<%= yield %>



			<!-- Footer -->
			<footer class="main-footer">
				<div class="inner-wrap">
					<div class="footer-top">
						<ul class="nav">
							<li></li>
						</ul>
					</div>
					<div class="footer-bottom">
						<div class="copyright">
							<%= image_tag "scielo/logo-scielo-signature.png" %>
						</div>
						<div class="address">
							<strong>SciELO - Scientific Electronic Library Online</strong>
							<br />
							Av. Onze de Junho, 269 - Vila Clementino 04041-050 São Paulo SP - Brasil
							<br />
							Tel.: (55 11) 5083-3639 - Email: scielo@scielo.org
						</div>
					</div>
				</div>
			</footer>
		</div>



		<!-- JS -->
		<script>
			$('.main-header .nav li a').click(function() {
				$(window).scrollTo($(this).attr('href'), 1000, {
					offset: -46
				});
			});
			$('body').scrollspy({ target: '.navbar-bottom', offset: 150 });
		</script>


		<!-- Google Analytics -->
		<script>
			var _gaq=[['_setAccount','UA-34822510-2'],['_trackPageview']];
			(function(d,t){var g=d.createElement(t),s=d.getElementsByTagName(t)[0];
			g.src=('https:'==location.protocol?'//ssl':'//www')+'.google-analytics.com/ga.js';
			s.parentNode.insertBefore(g,s)}(document,'script'));

			$('.main-header').affix({
				offset: {
					top: $('.main-header').height(),
					bottom: $('.main-footer').outerHeight(true)
				}
			});
		</script>



	</body>
</html>

